{
  "name": "aipscan",
  "description": "Crawl Archivematica AIPs to provide repository-wide reporting",
  "scripts": {
    "build": "run-p --print-label build:*",
    "build:base": "vite build -c build_config/base.config.js",
    "build:report": "vite build -c build_config/report_base.config.js",
    "build:chart": "vite build -c build_config/chart_formats_count.config.js",
    "build:plot": "vite build -c build_config/plot_formats_count.config.js"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^7.0.1",
    "@rollup/plugin-inject": "^5.0.5",
    "bootstrap": "^5.3.8",
    "chart.js": "^4.5.0",
    "jquery": "^3.7.1",
    "jquery-ui-dist": "^1.13.3",
    "npm-run-all": "^4.1.5",
    "plotly.js-dist": "^3.1.1",
    "vite": "^7.1.7"
  },
  "type": "module"
}
